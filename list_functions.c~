#include "main.h"

/**
 * add_snodeint - add a new node at the beginning
 * @head: doubly linked list
 * @n: data
 *
 * Return: address of new node or NULL
 */

stack_t *add_snodeint(stack_t **head, const int n)
{
	stack_t *new_node;

	new_node = malloc(sizeof(stack_t));
	if (new_node == NULL)
		return (NULL);

	new_node->n = n;
	new_node->next = *head;
	new_node->prev = NULL;
	if (*head)
		(*head)->prev = new_node;
	*head = new_node;

	return (new_node);
}
